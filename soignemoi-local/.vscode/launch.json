{
    "version": "0.2.0",
    "configurations": [
      {
        "name": "Listen for Xdebug",
        "type": "php",
        "request": "launch",
        "port": 9003,
        "pathMappings": {
          "/var/www/html/soignemoi-local/": "${workspaceFolder}"
        },
        "xdebugSettings": {
          "max_children": 512,
          "max_data": 1024,
          "max_depth": 5
        }
      }
    ]
  }